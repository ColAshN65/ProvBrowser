<UserControl x:Class="ProvBrowser.View.Components.BrowsersTabComponent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProvBrowser.View.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl x:Name="MainTab" DataContext="{Binding BrowsersCollection}" ItemsSource="{Binding Views}" Background="Black">
            
        </TabControl>
    </Grid>
</UserControl>
